<h1>Layout Nodes</h1>

<table class="responsive nowrap table table-hover" id="dttb-layout_nodes" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="direction">Direction</th>
      <th data-data="grid_x">Grid x</th>
      <th data-data="grid_y">Grid y</th>
      <th data-data="column">Column</th>
      <th data-data="row">Row</th>
      <th data-data="profile">Profile</th>
      <th data-data="node_kind">Node kind</th>
      <th data-data="tag">Tag</th>
      <th data-data="selected">Selected</th>
      <th data-data="actions">Actions</th>
      <th data-data="layout">Layout</th>
      <th data-data="layout_with_pillar_path">Layout with pillar path</th>
      <th data-data="box_count">Box count</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

