<%= simple_form_for(@combo_ad) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :base_ad %>
    <%= f.input :column %>
    <%= f.input :row %>
    <%= f.input :layout %>
    <%= f.input :profile %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
