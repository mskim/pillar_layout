<div class="card-columns">
  <% @yh_photo_trs.each do |yh_photo_tr| %>
    <div class="card text-center">
      <%= link_to (image_tag yh_photo_tr.thumb_path, class: "img-crop", style: "width: 100%;"), yh_photo_tr_path(yh_photo_tr), remote: true %>
      <div class="card-footer">
        <h6 class="card-title text-center mb-0"><%= yh_photo_tr.title %></h6>
        <small class="text-muted"><%= yh_photo_tr.created_at.strftime("%y-%m-%d %H:%M") %></small>
      </div>
    </div>
  <% end %>
</div>
