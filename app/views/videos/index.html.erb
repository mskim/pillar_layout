<h1>Videos</h1>

<table class="responsive nowrap table table-hover" id="dttb-videos" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="x">X</th>
      <th data-data="y">Y</th>
      <th data-data="width">Width</th>
      <th data-data="height">Height</th>
      <th data-data="player_type">Player type</th>
      <th data-data="source_video_url">Source video url</th>
      <th data-data="web_page">Web page</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Video', new_video_path %>