<%= simple_form_for(@page) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :page_number %>
    <%= f.input :section_name %>
    <%= f.input :column %>
    <%= f.input :row %>
    <%= f.input :ad_type %>
    <%= f.input :story_count %>
    <%= f.input :color_page %>
    <%= f.input :profile %>
    <%= f.association :issue %>
    <%= f.input :template_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
