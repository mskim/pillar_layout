<%= simple_form_for(@yh_picture) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :action %>
    <%= f.input :service_type %>
    <%= f.input :content_id %>
    <%= f.input :date %>
    <%= f.input :time %>
    <%= f.input :urgency %>
    <%= f.input :category %>
    <%= f.input :class_code %>
    <%= f.input :attriubute_code %>
    <%= f.input :source %>
    <%= f.input :credit %>
    <%= f.input :page_ref %>
    <%= f.input :title %>
    <%= f.input :comment %>
    <%= f.input :body %>
    <%= f.input :file_name %>
    <%= f.input :taken_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
