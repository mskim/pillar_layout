<h1>Annotations</h1>

<table class="responsive nowrap table table-hover" id="dttb-annotations" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="working_article">Working article</th>
      <th data-data="version">Version</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

