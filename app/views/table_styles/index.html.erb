<h1>Table Styles</h1>

<table class="responsive nowrap table table-hover" id="dttb-table_styles" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="name">Name</th>
      <th data-data="column">Column</th>
      <th data-data="row">Row</th>
      <th data-data="heading_level">Heading level</th>
      <th data-data="category_level">Category level</th>
      <th data-data="layout">Layout</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Table Style', new_table_style_path %>