<h1>Spread Ad Boxes</h1>

<table class="responsive nowrap table table-hover" id="dttb-spread_ad_boxes" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="ad_type">Ad type</th>
      <th data-data="advertiser">Advertiser</th>
      <th data-data="row">Row</th>
      <th data-data="width">Width</th>
      <th data-data="height">Height</th>
      <th data-data="spread">Spread</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Spread Ad Box', new_spread_ad_box_path %>