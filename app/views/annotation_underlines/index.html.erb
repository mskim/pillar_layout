<h1>Annotation Underlines</h1>

<table class="responsive nowrap table table-hover" id="dttb-annotation_underlines" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="x">X</th>
      <th data-data="y">Y</th>
      <th data-data="width">Width</th>
      <th data-data="height">Height</th>
      <th data-data="color">Color</th>
      <th data-data="annotation">Annotation</th>
      <th data-data="user">User</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Annotation Underline', new_annotation_underline_path %>