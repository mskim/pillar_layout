<%= simple_form_for(@page_layout) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :page_type %>
    <%= f.input :ad_type %>
    <%= f.input :column %>
    <%= f.input :row %>
    <%= f.input :layout, { multiple: true}%>
    <%= f.input :like %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
