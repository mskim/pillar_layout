<h1>Graphics</h1>

<table class="responsive nowrap table table-hover" id="dttb-graphics" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="grid_x">Grid x</th>
      <th data-data="grid_y">Grid y</th>
      <th data-data="column">Column</th>
      <th data-data="row">Row</th>
      <th data-data="extra_height_in_lines">Extra height in lines</th>
      <th data-data="graphic">Graphic</th>
      <th data-data="caption">Caption</th>
      <th data-data="source">Source</th>
      <th data-data="position">Position</th>
      <th data-data="page_number">Page number</th>
      <th data-data="story_number">Story number</th>
      <th data-data="working_article">Working article</th>
      <th data-data="issue_id">Issue</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Graphic', new_graphic_path %>