<h1>Section Headings</h1>

<table>
  <thead>
    <tr>
      <th>Page number</th>
      <th>Section name</th>
      <th>Date</th>
      <th>Layout</th>
      <th>Publication</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_headings.each do |section_heading| %>
      <tr>
        <td><%= section_heading.page_number %></td>
        <td><%= section_heading.section_name %></td>
        <td><%= section_heading.date %></td>
        <td><%= section_heading.layout %></td>
        <td><%= section_heading.publication_id %></td>
        <td><%= link_to 'Show', section_heading %></td>
        <td><%= link_to 'Edit', edit_section_heading_path(section_heading) %></td>
        <td><%= link_to 'Destroy', section_heading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section Heading', new_section_heading_path %>
