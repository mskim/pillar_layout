<%= simple_form_for(@section_heading) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :page_number %>
    <%= f.input :section_name %>
    <%= f.input :date %>
    <%= f.input :layout %>
    <%= f.input :publication_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
