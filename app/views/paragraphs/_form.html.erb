<%= simple_form_for(@paragraph) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :working_article %>
    <%= f.input :order %>
    <%= f.input :para_text %>
    <%= f.input :tokens %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
