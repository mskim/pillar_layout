<h1>Paragraphs</h1>

<table class="responsive nowrap table table-hover" id="dttb-paragraphs" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="name">Name</th>
      <th data-data="working_article">Working article</th>
      <th data-data="order">Order</th>
      <th data-data="para_text">Para text</th>
      <th data-data="tokens">Tokens</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Paragraph', new_paragraph_path %>