<h3>사진 선택</h3>
  <% @library_images.each do |image| %>
  <div class = "col-md-1"  align='left' style="padding-bottom: 40px;">
    <image src="<%= image %>" width="100%" height="100%"></img>
    <div align="center">
      <%= link_to "선택", select_image_article_path(@article, :selected_image=> image), method: :get, class: "btn btn-sm btn-danger" %>
    </div>
  </div>
  <% end %>
  <hr>
